    "format-check": "prettier --check \"**/*.(md|yaml|json)\"",
    "format": "prettier --write \"**/*.(md|yaml|json)\"",
    "yaml-validate:ranks": "ajv validate -s content/ranks.schema.json -d content/ranks.yaml",
    "yaml-validate:staff": "ajv validate -s content/staff.schema.json -d content/staff.yaml",
    "yaml-validate:commands": "ajv validate -s content/commands/commands.schema.json -d \"content/commands/**/!(commandList).yaml\"",
    "yaml-validate:locations": "ajv validate -s content/locations/locations.schema.json -d \"content/locations/**/!(locationList).yaml\"",
